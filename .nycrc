{
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.js",
    "src/**/*.ts"
  ],
  "exclude": [
    "test/**/*.spec.*"
  ],
  "require": [
    "ts-node/register"
  ],
  "sourceMap": true,
  "instrument": true,
  "check-coverage": false,
  "cache": true
}
