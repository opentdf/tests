{
  "name": "tdf3-js",
  "version": "4.1.8",
  "description": "Minimal library to read and write files in the TDF3 spec.",
  "homepage": "https://github.com/virtru/tdf3-js#readme",
  "bugs": {
    "url": "https://github.com/virtru/tdf3-js/issues"
  },
  "files": [
    "build/index.js",
    "build/src/**",
    "src/**",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opentdf/tdf3-js.git"
  },
  "license": "MIT",
  "author": "Virtru",
  "main": "./build/index.js",
  "module": "./dist/src/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run build:js && npm run build:browser",
    "build:browser": "browserify --debug --standalone tdf3 ./build/index.js -o build/bundle-dev.js && browserify --standalone tdf3 ./build/index.js -o build/bundle.js",
    "build:ts": "tsc -d",
    "build:js": "babel --out-dir build/ --extensions '.js,.ts' --source-maps inline --only 'src/**','test/**','./index.js' .",
    "clean": "rm -rf {build,dist}",
    "pretty": "prettier --write \"./**/*.{css,md,js,json,ts}\"",
    "test": "nyc mocha --require @babel/register --timeout 300000",
    "test:browser-headless": "karma start --single-run --browsers ChromeHeadlessNoSandbox karma.conf.cjs",
    "lint": "eslint ./{src,test}",
    "lint:fix": "eslint --fix ./{src,test}",
    "prepack": "NODE_ENV=production npm run build"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index -g",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.18.0",
    "@aws-sdk/lib-storage": "^3.18.0",
    "@babel/runtime": "^7.16.7",
    "@babel/runtime-corejs3": "^7.17.2",
    "axios": "~0.24.0",
    "bluebird": "~3.7.2",
    "buffer-crc32": "^0.2.13",
    "combined-stream": "^1.0.8",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "file-saver": "^2.0.5",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "~8.5.1",
    "node-forge": "^1.2.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "path-browserify": "^1.0.1",
    "query-string": "^7.1.1",
    "sjcl": "github:virtru/sjcl#v1.0.4+virtru.2016.08.23",
    "stream-browserify": "^3.0.0",
    "streamsaver": "^2.0.5",
    "ua-parser-js": "0.7.28",
    "utf8": "^3.0.0",
    "uuid": "~8.3.2",
    "web-streams-node": "^0.4.0",
    "web-streams-polyfill": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@types/mocha": "^9.0.0",
    "@types/node-forge": "1.0.0",
    "@types/sjcl": "^1.0.30",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "audit-ci": "^5.1.2",
    "babel-loader": "^8.2.3",
    "browserify": "~17.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "colors": "^1.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jszip": "^3.7.1",
    "karma": "^6.3.17",
    "karma-browserify": "^8.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.3",
    "karma-typescript-es6-transform": "^5.5.3",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "send": "^0.17.1",
    "sinon": "^11.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsify": "^5.0.4",
    "typescript": "^4.5.4",
    "webpack": "^5.58.2"
  }
}
